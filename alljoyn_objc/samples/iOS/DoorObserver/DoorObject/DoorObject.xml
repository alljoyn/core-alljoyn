<xml>
    <node name="com/example/Door">
        <annotation name="org.alljoyn.lang.objc" value="DoorObject"/>
        <interface name="com.example.Door">
            <annotation name="org.alljoyn.lang.objc" value="DoorObjectDelegate"/>
            <annotation name="org.alljoyn.lang.objc.announced" value = "true"/>

            <property name="IsOpen" type="b" access="read">
                <annotation name="org.freedesktop.DBus.Property.EmitsChangedSignal" value="true"/>
            </property>
            <property name="Location" type="s" access="read">
                <annotation name="org.freedesktop.DBus.Property.EmitsChangedSignal" value="true"/>
            </property>
            <property name="KeyCode" type="u" access="read">
                <annotation name="org.freedesktop.DBus.Property.EmitsChangedSignal" value="invalidates"/>
            </property>

            <method name="Open"/>
            <method name="Close"/>
            <method name="KnockAndRun">
                <annotation name="org.freedesktop.DBus.Method.NoReply" value="true"/>
            </method>

            <signal name="PersonPassedThrough">
                <arg name="name" type="s">
                    <annotation name="org.alljoyn.lang.objc" value="PersonPassedThroughName:"/>
                </arg>
            </signal>
        </interface>
    </node>
</xml>